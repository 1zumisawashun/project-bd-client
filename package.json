{
  "name": "project-bd-client",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint:test": "npx eslint",
    "oxlint:test": "npx oxlint & npx eslint",
    "lint:js": "eslint 'app/**/*.{tsx,ts}' --cache",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:watch": "npx eslint --inspect-config",
    "custom-rules:watch": "npx tsc -p ./eslint-plugin-custom-rules/tsconfig.json --watch",
    "format": "prettier \"app/**/*.+(js|json|yml|ts|tsx)\" --cache --cache-location=.prettiercache",
    "format:fix": "npm run format -- --write",
    "compile": "npx tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma db push --force-reset && npm run prisma:seed",
    "vercel-build": "prisma generate && next build && prisma migrate deploy"
  },
