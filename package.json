{
  "name": "project-bd-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --ext .tsx,.ts app/",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:clean": "rimraf ./eslint-plugin-custom-rules/rules/",
    "lint:js:build": "npm run lint:js:clean && npx tsc -p ./eslint-plugin-custom-rules/tsconfig.json",
    "format": "prettier \"app/**/*.+(js|json|yml|ts|tsx)\"",
    "format:fix": "prettier --write \"app/**/*.+(js|json|yml|ts|tsx)\"",
    "compile": "npx tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma db push --force-reset && npm run prisma:seed",
    "vercel-build": "prisma generate && next build && prisma migrate deploy"
  },
